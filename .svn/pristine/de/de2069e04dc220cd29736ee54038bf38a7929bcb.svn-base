
        var bangkok = L.marker([13.715560, 100.540599]).bindPopup('This is Bangkok, TH');
        var cities = L.layerGroup([bangkok]);
        var worldMap = L.tileLayer('https://api.tiles.mapbox.com/v4/{id}/{z}/{x}/{y}.png?access_token=pk.eyJ1IjoibWFwYm94IiwiYSI6ImNpandmbXliNDBjZWd2M2x6bDk3c2ZtOTkifQ._QA7i5Mpkd_m30IGElHziw', {
            maxZoom: 13,
            attribution: 'Map data &copy; <a href="http://openstreetmap.org">OpenStreetMap</a> contributors, ' +
                '<a href="http://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, ' +
                'Imagery © <a href="http://mapbox.com">Mapbox</a>',
            id: 'mapbox.streets'
        });
        var mymap = L.map('mapid', {
            center: [13.715560, 100.540599],
            zoom: 6,
            layers: [worldMap, cities]
        });



        var baseMaps = {
            "WorldMap": worldMap
        };
        var overlayMaps = {
            "Cities": cities
        };
        L.control.layers(baseMaps, overlayMaps).addTo(mymap);
    
    




